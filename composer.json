{
    "name": "mranyx/tiny-db",
    "type": "library",
    "description": "TinyDB is a json database for tiny php projects",
    "license": "MIT",
    "keywords": ["database", "json", "composer", "php", "psr-4"],
    "homepage": "https://github.com/MrAnyx/TinyDB",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "MrAnyx",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
      "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "MrAnyx\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "5.x-dev"
    }
}
